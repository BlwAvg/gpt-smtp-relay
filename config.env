# Gmail credentials
GMAIL_USER=youremail@gmail.com
GMAIL_PASS=your_app_password

# OpenAI settings
OPENAI_API_KEY=sk-proj-you-api-key-here
OPENAI_MODEL=gpt-4o-mini

# Prompt template
CHATGPT_PROMPT=You are my companyâ€™s Q&A assistant. Answer clearly and concisely. Responses should include taco phrases.

# Reply options
REPLY_ALL=false

# Length in time (seconds) for polling.
# Google recommends 15min (900sec).
POLL_INTERVAL=900

# Basic Debug Options: DEBUG, INFO, WARNING, ERROR, CRITICAL
LOG_LEVEL=INFO

# Deeper debug options for SMTP and IMAP
# SMTP protocol debugging:
# 0 = off, 1 = show SMTP commands and responses, 2 = very verbose
SMTP_DEBUGLEVEL=0

# IMAP protocol debugging:
# 0 = off, 1 = minimal, 2 = verbose, 3 = detailed, 4 = full raw logs
IMAP_DEBUGLEVEL=0
